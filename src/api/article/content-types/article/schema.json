{
    "kind": "collectionType",
    "collectionName": "articles",
    "info": {
        "singularName": "article",
        "pluralName": "articles",
        "displayName": "Artyku≈Ç"
    },
    "options": {
        "draftAndPublish": true
    },
    "attributes": {
        "title": {
            "type": "string",
            "required": true
        },
        "content": {
            "type": "richtext"
        },
        "readTime": {
            "type": "integer"
        },
        "thumbnail": {
            "type": "media",
            "allowedTypes": [
                "images"
            ],
            "multiple": false
        },
        "profiles": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::profile.profile"
        },
        "author": {
            "type": "string"
        },
        "tag": {
            "type": "string"
        }
    }
}